{
  "type": "project",
  "require": {
    "php": "^8.2"
  },
  "require-dev": {
      "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
        "App\\": "src/"
    }
  },
  "autoload-dev": {
      "psr-4": {
          "App\\Tests\\": "tests/"
      }
  },
  "scripts": {
    "test": "./vendor/bin/phpunit --testdox --colors=auto"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}


 
  
  
